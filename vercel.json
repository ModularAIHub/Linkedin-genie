{
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install && cd ../server && npm install",
  "devCommand": "npm run dev",
  "framework": null,
  "functions": {
    "../server/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/../server/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}