{
  "buildCommand": "npm run build",
  "outputDirectory": "client/dist",
  "installCommand": "npm run install-all",
  "framework": null,
  "functions": {
    "server/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}